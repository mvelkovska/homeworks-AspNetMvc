@model List<SEDC.PizzaApp.Models.Domain.Order> 
    
    @*precekuvame lista od naracki*@

   

    <h2> List of all the orders: </h2>

<ul>
    @for(int i = 0; i < Model.Count(); i++)
    {
        <li>
        <p>@Model[i].Customer - <strong>@Model[i].typeOfPizza</strong></p>
            </li>
    }

    </ul>





